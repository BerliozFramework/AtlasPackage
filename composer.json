{
  "name": "berlioz/atlas-package",
  "type": "berlioz-package",
  "description": "Atlas.ORM package for Berlioz Framework",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "homepage": "https://getberlioz.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan@getberlioz.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\Package\\Atlas\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\Package\\Atlas\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ElGigi/Atlas.Orm.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ElGigi/Atlas.Table.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ElGigi/Atlas.Transit.git"
    }
  ],
  "require": {
    "php": "^8.0",
    "berlioz/cli-core": "^2.0",
    "atlas/orm": "^3.1",
    "atlas/cli": "^2.2",
    "atlas/transit": "0.x-dev",
    "doctrine/sql-formatter": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "berlioz": {
      "package": "Berlioz\\Package\\Atlas\\BerliozPackage"
    }
  }
}
